ggplot(UC_admit, aes_string(x = "Ethnicity", y="FilteredCountFR", fill=input$fill)) +
geom_col(position = "dodge") + theme_light(base_size = 18) +
coord_flip()+
labs(y="Count")
})
session$onSessionEnded(stopApp)
}
